{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "ES2022",
    "useDefineForClassFields": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],

    /* Bundler mode */
    "jsx": "react-jsx",

    /* Linting */
    "noUncheckedSideEffectImports": true
  },
  "include": ["src"]
}
